local Section = TabTwo:AddSection({
    Name = "Auto Hatch Fast"
})
    _G.autoHatch = true
    _G.selectEgg = "Basic Egg"

    function autoHatch()
        while _G.autoHatch == true do
game:GetService("ReplicatedStorage"):FindFirstChild("SSG Framework").Shared.Network:FindFirstChild("buy egg"):InvokeServer(_G.selectEgg,3)


            wait(0)
        end
    end
TabTwo:AddToggle({
Name = "Auto Hatch",
    Default = false,
    Callback = function(Value)
                _G.autoHatch = Value
                    autoHatch()
                    end
        })

TabTwo:AddDropdown({
	Name = "Choose Eggs",
